	urldate = {2022-02-01},
	journal = {ACM Transactions on Intelligent Systems and Technology},
	author = {Sharma, Karishma and Qian, Feng and Jiang, He and Ruchansky, Natali and Zhang, Ming and Liu, Yan},
	month = may,
	year = {2019},
	pages = {1--42},
	file = {Sharma et al. - 2019 - Combating Fake News A Survey on Identification an.pdf:/Users/derekpowell/Zotero/storage/7EXFHRYQ/Sharma et al. - 2019 - Combating Fake News A Survey on Identification an.pdf:application/pdf},
