@article{ecker.etal2011,
	title = {Correcting false information in memory: {Manipulating} the strength of misinformation encoding and its retraction},
	volume = {18},
	issn = {1069-9384, 1531-5320},
	shorttitle = {Correcting false information in memory},
	url = {http://link.springer.com/10.3758/s13423-011-0065-1},
	doi = {10.3758/s13423-011-0065-1},
	abstract = {Information that is presumed to be true at encoding but later on turns out to be false (i.e., misinformation) often continues to influence memory and reasoning. In the present study, we investigated how the strength of encoding and the strength of a later retraction of the misinformation affect this continued influence effect. Participants read an event report containing misinformation and a subsequent correction. Encoding strength of the misinformation and correction were orthogonally manipulated either via repetition (Experiment 1) or by imposing a cognitive load during reading (Experiment 2). Results suggest that stronger retractions are effective in reducing the continued influence effects associated with strong misinformation encoding, but that even strong retractions fail to eliminate continued influence effects associated with relatively weak encoding. We present a simple computational model based on random sampling that captures this effect pattern, and conclude that the continued influence effect seems to defy most attempts to eliminate it.},
	language = {en},
	number = {3},
	urldate = {2021-03-12},
	journal = {Psychonomic Bulletin \& Review},
	author = {Ecker, Ullrich K. H. and Lewandowsky, Stephan and Swire, Briony and Chang, Darren},
	month = jun,
	year = {2011},
	pages = {570--578},
}

